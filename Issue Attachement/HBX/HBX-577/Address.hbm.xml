<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <joined-subclass extends="de.infracore.hbm.Entity" name="de.infracore.hbm.Address" table="address" catalog="">
        <key column="id"/>
        <property name="country" type="string" unique="false" optimistic-lock="true" lazy="false" generated="never">
            <column name="country" length="40"/>
        </property>
        <property name="state" type="string" unique="false" optimistic-lock="true" lazy="false" generated="never">
            <column name="state" length="40"/>
        </property>
        <property name="postcode" type="string" unique="false" optimistic-lock="true" lazy="false" generated="never">
            <column name="postcode" length="20"/>
        </property>
        <property name="city" type="string" unique="false" optimistic-lock="true" lazy="false" generated="never">
            <column name="city" length="80"/>
        </property>
        <property name="street" type="string" unique="false" optimistic-lock="true" lazy="false" generated="never">
            <column name="street" length="80"/>
        </property>
        <property name="telPrivat" type="string" unique="false" optimistic-lock="true" lazy="false" generated="never">
            <column name="tel_privat" length="40"/>
        </property>
        <property name="telMobil" type="string" unique="false" optimistic-lock="true" lazy="false" generated="never">
            <column name="tel_mobil" length="40"/>
        </property>
        <property name="telBusiness" type="string" unique="false" optimistic-lock="true" lazy="false" generated="never">
            <column name="tel_business" length="40"/>
        </property>
	</joined-subclass>
</hibernate-mapping>
