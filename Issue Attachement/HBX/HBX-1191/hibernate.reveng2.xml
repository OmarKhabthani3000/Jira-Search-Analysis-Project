<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >
<hibernate-reverse-engineering>
	<table-filter match-name="parent"></table-filter>
	<table-filter match-name="child"></table-filter>
	<table-filter match-name="child2"></table-filter>
	<table name="parent">
		<column name="id"></column>
	</table>
	<table name="child">
		<column name="id"></column>
		<foreign-key constraint-name="child_parent_id_fk">
			<many-to-one exclude="true" />
			<set exclude="true" />
		</foreign-key>
		<foreign-key constraint-name="hib_child_parent_id_fk"
			foreign-table="parent">
			<column-ref foreign-column="id" local-column="parent_id" />
			<one-to-one exclude="false" />
		</foreign-key>
	</table>
	<table name="child2">
		<column name="id"></column>
		<foreign-key constraint-name="child2_parent_id_fk">
			<many-to-one exclude="true" />
			<set exclude="true" />
		</foreign-key>
		<foreign-key constraint-name="hib_child2_parent_id_fk"
			foreign-table="parent">
			<column-ref foreign-column="id" local-column="parent_id" />
			<one-to-one exclude="false" />
		</foreign-key>
	</table>
</hibernate-reverse-engineering>