<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Configuration DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.llic.ides">
  <class name="LapseRate" table="LapseRate">
    
   <meta attribute="implement-equals">true</meta>
   <meta attribute="generated-class">com.llic.ides.base.LapseRateBase</meta>
   
    <id name="id" column="lapseRateId" type="java.lang.Long">
      <meta attribute="use-in-equals">true</meta>
      
      <generator class="native" />
      
    </id>
    
  
    <property name="daysLeft" column="daysLeft" type="int" not-null="true"/>
    
    <property name="lapseEstimate" column="lapseEstimate" type="double" not-null="true"/>
    
    <property name="lapseDist" column="lapseDist" type="double" not-null="true"/>
   
    	<primitive-array name="lapseRates" table="lapseRates">
			<key column="lapseRateId"/>
			<index column="indexId"/>
			<element type="double" column="lapseRate"/>
	 	</primitive-array>
      
  </class>
</hibernate-mapping>

