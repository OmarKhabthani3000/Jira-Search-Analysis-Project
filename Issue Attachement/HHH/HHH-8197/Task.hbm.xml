<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.cg.oneToMany.model">

<class entity-name="_testCatalog_Task" table="TASK">
<id column="id" name="id" type="org.hibernate.type.StringType">
</id>
<property column="name" name="name" type="org.hibernate.type.StringType"/>
<set cascade="none" inverse="true" name="subtasks">
<key column="ownerId"/>
<one-to-many entity-name="_testCatalog_Subtask"/>
</set>
</class>
</hibernate-mapping>
