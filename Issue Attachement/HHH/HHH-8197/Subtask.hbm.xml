<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.cg.oneToMany.model">

    <class entity-name="_testCatalog_Subtask" table="SUBTASK">
        <id column="id" name="id" type="org.hibernate.type.StringType">
        </id>
        <property column="ownerId" name="ownerId" type="org.hibernate.type.StringType"/>
        <property column="label" name="label" type="org.hibernate.type.StringType"/>
        <property column="effort" name="effort" type="org.hibernate.type.IntegerType"/>
        <property column="assigneeId" name="assigneeId" type="org.hibernate.type.StringType"/>
        <set cascade="none" inverse="true" name="activities">
            <key column="subtaskId"/>
            <one-to-many entity-name="_testCatalog_Activity"/>
        </set>
        <many-to-one cascade="none" entity-name="_testCatalog_Assignee" insert="false" name="assignee" update="false">
            <column name="assigneeId"/>
        </many-to-one>
    </class>
</hibernate-mapping>
