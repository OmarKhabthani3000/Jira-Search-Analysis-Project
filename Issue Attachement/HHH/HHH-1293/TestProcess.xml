<?xml version="1.0" encoding="UTF-8"?>
<p:process-definition name="TestProcess" xmlns:p="urn:jbpm.org:jpdl-3.1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="urn:jbpm.org:jpdl-3.1 urn:jbpm.org:jpdl-3.1 ">
	<p:start-state name="start">
		<p:transition to="first-task"/>
	</p:start-state>
	
	<p:task-node name="first-task">
		<p:task name="first-task">
			<p:assignment pooled-actors="indexers"/>
		</p:task>
		<p:transition to="end"/>
	</p:task-node>
	
	<p:end-state name="end">
	</p:end-state>
</p:process-definition>
