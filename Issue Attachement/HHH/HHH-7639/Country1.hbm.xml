<?xml version="1.0" encoding="UTF-8"?>
    <!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
    
<hibernate-mapping package="com.wakaleo.articles.caching.businessobjects">
    <class entity-name="Country1" table="COUNTRY1" dynamic-insert="true" mutable="true" polymorphism="implicit" dynamic-update="false" select-before-update="false" optimistic-lock="version"> 
 
        <cache usage="read-only"/>  


        <id name="id" type="java.lang.Long" column="cn_id">
            <generator class="native"/>
        </id>

        <property column="cn_code" name="code" type="string" optimistic-lock="true" lazy="false" generated="never"/>
        <property column="cn_name" name="name" type="string" optimistic-lock="true" lazy="false" generated="never"/>
 
    </class>
</hibernate-mapping>
   