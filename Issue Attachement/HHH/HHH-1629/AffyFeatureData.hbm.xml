<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping
>
    <class
        name="net.sf.basedb.core.data.AffyFeatureData"
        table="`AffyFeatures`"
        lazy="false"
        dynamic-update="false"
        dynamic-insert="false"
        select-before-update="false"
        optimistic-lock="version"
    >

        <id
            name="id"
            column="`id`"
            type="int"
            unsaved-value="0"
        >
            <generator class="native">
		<param name="sequence">feature_sequence</param>

            </generator>
        </id>

        <version
            name="version"
            type="int"
            column="`version`"
            access="property"
            unsaved-value="undefined"
        />

        <property
            name="mismatch"
            type="boolean"
            update="false"
            insert="true"
            access="property"
            column="`mismatch`"
            not-null="true"
        />

        <property
            name="ordinal"
            type="int"
            update="false"
            insert="true"
            access="property"
            column="`ordinal`"
            not-null="true"
        />

        <property
            name="sequence"
            type="string"
            update="false"
            insert="true"
            access="property"
            column="`sequence`"
            length="255"
            not-null="false"
        />

        <property
            name="column"
            type="int"
            update="false"
            insert="true"
            access="property"
            column="`column`"
            not-null="true"
        />

        <property
            name="row"
            type="int"
            update="false"
            insert="true"
            access="property"
            column="`row`"
            not-null="true"
        />

        <property
            name="position"
            type="int"
            update="false"
            insert="true"
            access="property"
            column="`position`"
            not-null="true"
        />

        <many-to-one
            name="arrayDesignBlock"
            class="net.sf.basedb.core.data.ArrayDesignBlockData"
            cascade="none"
            fetch="select"
            update="false"
            insert="true"
            access="property"
            column="`arraydesignblock_id`"
            not-null="true"
        />

        <many-to-one
            name="well"
            class="net.sf.basedb.core.data.WellData"
            cascade="none"
            fetch="select"
            update="false"
            insert="true"
            access="property"
            column="`well_id`"
            not-null="false"
        />

		<many-to-one
			name="reporter"
			class="net.sf.basedb.core.data.ReporterData"
			cascade="evict"
			fetch="select"
			update="false"
			insert="true"
			access="property"
			column="`reporter_id`"
			not-null="false"
			lazy="proxy"
		/>

    </class>

</hibernate-mapping>
