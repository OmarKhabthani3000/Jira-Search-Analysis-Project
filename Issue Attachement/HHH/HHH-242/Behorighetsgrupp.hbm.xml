<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="daisy.shared.enhet.behorighet.Behorighetsgrupp" table="Behörighetsgrupp" lazy="false">
		<id name="id" column="GruppID" type="long" unsaved-value="0">
			<generator class="native"/>
		</id>

		<set name="behorigheter" lazy="false" table="Grupp_Behörighet" sort="natural">
			<key column="Grupp_ID"/>
			<element column="Behörighet" type="string" not-null="true"/>
		</set>

	</class>
</hibernate-mapping>