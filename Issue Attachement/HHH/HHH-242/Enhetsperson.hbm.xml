<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="daisy.shared.enhet.Enhetsperson" table="Enhetsperson" lazy="false">
		<id name="id" type="long" column="EnhetspersonID" unsaved-value="0">
			<generator class="native"/>
		</id>
		<property name="personID" column="Person_ID" type="long"/>
		<set name="behorighetsgrupper" lazy="true" cascade="none" inverse="true" table="Pers_Behörighetsgrupp">
			<key column="Enhetsperson_ID"/>
			<many-to-many class="daisy.shared.enhet.behorighet.Behorighetsgrupp" column="Grupp_ID"/>
		</set>
	</class>
</hibernate-mapping>