<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class lazy="false"  name="com.logistics.so.rts.bo.RTS" table="RTS">
<composite-id name="id" class="com.logistics.so.rts.bo.RTSPK">
	<key-property name="rtsId" column="RTS_ID" type="integer"/>
</composite-id>
<version column="VERSION" name="version" type="long"/>
<property column="TC_COMPANY_ID" length="8" name="tcCompanyId" not-null="true" type="integer"/>
<property column="RTS_O_FACILITY_ID" length="8" name="rtsOFacilityIdInteger" type="integer"/>
<property column="RTS_D_FACILITY_ID" length="8" name="rtsDFacilityIdInteger" type="integer"/>
<property column="RTS_O_FACILITY_ALIAS_ID" length="16" name="rtsOFacilityAliasId" type="string"/>
<property column="RTS_D_FACILITY_ALIAS_ID" length="16" name="rtsDFacilityAliasId" type="string"/>
<property column="IS_RTS_ORIGIN_CHECKED" length="1" name="isRtsOriginChecked" not-null="true" type="byte"/>
<property column="IS_RTS_DESTINATION_CHECKED" length="1" name="isRtsDestinationChecked" not-null="true" type="byte"/>
<property column="PICKUP_START_DTTM" length="7" name="pickupStartDttm" type="timestamp"/>
<property column="PICKUP_END_DTTM" length="7" name="pickupEndDttm" type="timestamp"/>
<!-- New Column added PICKUP_TZ and DELIVERY_TZ -->
<property column="PICKUP_TZ" name="pickupTimeZone" type="integer"/>
<property column="DELIVERY_TZ" name="deliveryTimeZone" type="integer"/>
<!-- End of Column added PICKUP_TZ and DELIVERY_TZ -->
<property column="DELIVERY_START_DTTM" length="7" name="deliveryDttmStart" type="timestamp"/>
<property column="DELIVERY_END_DTTM" length="7" name="deliveryDttmEnd" type="timestamp"/>
<property column="BUSINESS_PARTNER_ID" length="8" name="businessPartnerId" type="string"/>
<property column="TOTAL_SIZE_ON_ORDERS" length="13" name="totalSizeOnOrders" not-null="true" type="double"/>
<property column="ADDITIONAL_SIZE_1" length="16" name="additionalSize1Double" type="double"/>
<property column="ADDITIONAL_SIZE_1_UOM_ID" length="8" name="additionalSize1UomIdInteger" type="integer"/>
<property column="ADDITIONAL_SIZE_2" length="16" name="additionalSize2Double" type="double"/>
<property column="ADDITIONAL_SIZE_2_UOM_ID" length="8" name="additionalSize2UomIdInteger" type="integer"/>
<property column="QUANTITY" length="16" name="quantityDouble" type="double"/>
<property column="QUANTITY_UOM_ID" length="8" name="quantityUomIdInteger" type="integer"/>
<property column="RTS_STATUS" length="2" name="rtsStatus" not-null="true" type="short"/>
<property column="IS_LOCKED" length="1" name="isLocked" not-null="true" type="byte"/>
<property column="RRC_NUMBER" length="9" name="rrcNumber" type="string"/>
<property column="IS_HAZMAT" length="1" name="isHazmat" not-null="true" type="byte"/>
<property column="AUTO_CREATE_METHOD" length="2" name="autoCreateMethod" not-null="true" type="integer"/>
<property column="CREATED_DTTM" length="7" name="createdDttm" not-null="true" type="timestamp"/>
<property column="CREATED_SOURCE" length="25" name="createdSource" not-null="true" type="string"/>
<property column="MODIFIED_DTTM" length="7" name="modifiedDttm" not-null="true" type="timestamp"/>
<property column="MODIFIED_SOURCE" length="25" name="modifiedSource" not-null="true" type="string"/>
<property column="CREATED_SOURCE_TYPE" length="25" name="createdSourceType" not-null="true" type="string"/>
<property column="MODIFIED_SOURCE_TYPE" length="25" name="modifiedSourceType" not-null="true" type="string"/>
<!-- New added for EDI ST02 -->
<property column="TS_CTRL_NUMBER" length="50" name="tsCrtlNumber" type="string"/>
<set name="rtsLineItems" inverse="true" cascade="all-delete-orphan" fetch="join" lazy="false">
	<key column="RTS_ID"/>
	<one-to-many class="com.logistics.so.rts.bo.RTSLineItem"/>
</set>
</class>
</hibernate-mapping>
