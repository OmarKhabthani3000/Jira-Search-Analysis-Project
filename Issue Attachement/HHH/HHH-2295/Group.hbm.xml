<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.fares.mtrac.group.Group" entity-name="lender-group" table="lender_group_name">
    <id name="id" type="string" column="group_cd">
      <generator class="com.fares.mtrac.group.hibernate.GroupIdSequenceGenerator">
        <param name="sequence">lender_seq</param>
      </generator>
    </id>
    <property name="name" column="group_nm" type="string" not-null="true" />
    <property name="userName" column="user_nm" type="string" not-null="true" />
    <set name="companyIds" table="lender_group" cascade="delete,delete-orphan" lazy="false">
      <key column="group_cd" />
      <element column="lndr_cd" type="string" />
    </set>    
  </class>
  
  <class name="com.fares.mtrac.group.Group" entity-name="titlegroup" table="title_company_group_name">
    <id name="id" type="string" column="group_cd">
      <generator class="com.fares.mtrac.group.hibernate.GroupIdSequenceGenerator">
        <param name="sequence">title_seq</param>
      </generator>
    </id>
    <property name="name" column="group_nm" type="string" not-null="true" />
    <property name="userName" column="user_nm" type="string" not-null="true" />
    <set name="companyIds" table="title_company_group" cascade="delete,delete-orphan" lazy="false">
      <key column="group_cd" />
      <element column="title_co_cd" type="string" />
    </set>    
  </class>
</hibernate-mapping>