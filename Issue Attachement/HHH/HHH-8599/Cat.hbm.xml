<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="CAT" name="com.company.product.domain.Cat">
    <id name="id" type="long" column="ID">
      <generator class="assigned"></generator>
    </id>
    <property name="name" type="string">
      <column name="NAME" unique-key="UK_ANIMAL" />
    </property>
    <set name="owners" table="CAT_PERSON" lazy="false">
      <key>
        <column name="ID_CAT" />
      </key>
      <many-to-many class="com.company.product.domain.Person" column="ID_PERSON" />
    </set>
    <map name="map" table="MAP">
      <key column="CAT_KEY" />
      <map-key type="java.lang.Integer">
        <column name="MAP_KEY" />
      </map-key>
      <element column="MAP_VALUE" type="java.lang.Integer" />
    </map>
  </class>
</hibernate-mapping>