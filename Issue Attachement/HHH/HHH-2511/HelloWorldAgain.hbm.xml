<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="HelloWorldAgain" table="HELLO_WORLD">
    
        <id name="id" column="HELLO_WORLD_ID">
            <generator class="increment"/>
        </id>
        
        <property name="modified"
			type="timestamp"
			column="CREATED"
			generated="insert"
			insert="false"
			update="false"/>
        
    </class>

</hibernate-mapping>