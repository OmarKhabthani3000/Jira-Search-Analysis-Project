<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.imsweb.seerdms.shared.entity.RecordRegistry" 
		table="RECORD_REGISTRY"
		dynamic-insert="true" lazy="false">
		
		<id name="recordId" type="java.lang.Long" column="RECORD_ID">
			<generator class="foreign">
				<param name="property">record</param>
			</generator>
		</id>

	<version name="version" column="VERSION" type="java.lang.Long"/>

	<property name="dateMammogramYear" column="DATE_MAMMOGRAM_YYYY" type="java.lang.String" length="4"/>
	<property name="dateMammogramMonth" column="DATE_MAMMOGRAM_MM" type="java.lang.String" length="2"/>
	<property name="dateMammogramDay" column="DATE_MAMMOGRAM_DD" type="java.lang.String" len

	<one-to-one name="record" class="com.imsweb.seerdms.shared.entity.Record" constrained="true"/>

	</class>
	
</hibernate-mapping>
