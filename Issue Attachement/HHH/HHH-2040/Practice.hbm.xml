<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Configuration DTD 3.0/EN" 
"file://primeschedule/hibernate/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="primeschedule.hibernate.Practice" table="practice">
        <id name="id" column="PRACTICE_ID" type="long">
            <generator class="native"/>
        </id> 
        <property name="name" type="string" column="NAME" length="128"/>
    </class> 
</hibernate-mapping>

