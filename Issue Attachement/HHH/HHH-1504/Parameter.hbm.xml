<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
    <typedef class="com.j2fe.hibernate.EnumType" name="com.j2fe.workflow.definition.ParameterType">
        <param name="type">com.j2fe.workflow.definition.ParameterType</param>
    </typedef>
    <typedef class="com.j2fe.hibernate.ClassType" name="java.lang.Class"/>
    <class name="com.j2fe.workflow.definition.Parameter" table="FT_O_WFPM" mutable="true" >
<!--
        <meta attribute="implement-equals">true</meta>
-->
        <!-- Cached internally already my workflow framework
        <cache usage="read-only"/>-->
        <id name="id" type="string">
            <generator class="com.j2fe.hibernate.OIDGenerator">
                <param name="TemplateGenerator">TransactionIdTemplateGenerator</param>
            </generator>
        </id>
        <property name="name" type="string" not-null="true" column="name" update="false"/>
        <property name="stringValue" type="string" not-null="false" column="string_value" update="false"/>
        <property name="objectValue" type="com.j2fe.hibernate.SerializableType" not-null="false" column="binary_value" update="false">
            <meta attribute="property-type">Object</meta>
        </property>
        <property name="type" column="type" type="com.j2fe.workflow.definition.ParameterType" not-null="true" update="false"/>
        <property name="UITypeHint" column="classname" type="string" not-null="false" update="false"/>
        <property name="input" type="yes_no" column="input" not-null="true" update="false"/>
        <property name="variablePart" type="string" column="part" not-null="false" update="false"/>
    </class>
</hibernate-mapping>