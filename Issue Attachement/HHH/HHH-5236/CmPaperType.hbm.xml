<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>

	<class name="com.shutterfly.commercial.model.CmPaperType" table="CMPAPERTYPE"
		lazy="false">
		<id name="ID" type="java.lang.Integer" column="MID">
			<generator class="sequence">
				<param name="sequence">CMPAPERTYPE_SEQ</param>
			</generator>
		</id>

		<property name="paperType" type="java.lang.String" column="PAPERTYPE" />
		<property name="weight" type="java.lang.String" column="WEIGHT" />
		<property name="category" type="java.lang.String" column="CATEGORY" />
		<property name="insertTime" type="java.sql.Timestamp" column="INSERTTIME" />
		<property name="active" type="java.lang.String" column="ACTIVE" />
		<property name="description" type="java.lang.String" column="DESCRIPTION" />

	</class>
</hibernate-mapping>
