<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping package="mil.navy.med.nmcsd.icdb.model.pophealth">

    <query name="pophealth.Alert.by.patID.diseaseID.status.type"><![CDATA[
        from Alert as alert
            where alert.patID = :patID
            and alert.diseaseID = :diseaseID
            and alert.status = :status
            and alert.type = :type
    ]]></query>

</hibernate-mapping>
