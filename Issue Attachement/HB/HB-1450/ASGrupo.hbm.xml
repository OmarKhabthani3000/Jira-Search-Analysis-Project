<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >

<hibernate-mapping>

  <!--
    Created by GEDI 1.0 28-02-2005 15:20:15
  -->

  <class
    name="pt.gedi.siag.persistence.ASGrupo"
    table="asgrupo"
    lazy="true">

    <id
      name="chvP"
      type="java.lang.Integer"
      column="ChvP">
      <generator class="native" />
    </id>

    <property
      name="designacao"
      type="java.lang.String"
      column="Designacao"
      length="40"
      not-null="true"
    />

    <property
      name="grupoFiltro"
      type="java.lang.Integer"
      column="GrupoFiltro"
      length="11"
    />

    <property
      name="criacaoTS"
      type="java.sql.Timestamp"
      column="CriacaoTS"
      length="14"
      not-null="true"
    />

    <property
      name="criacaoUtilizador"
      type="java.lang.Integer"
      column="CriacaoUtilizador"
      length="11"
      not-null="true"
    />

    <property
      name="UAltTS"
      type="java.sql.Timestamp"
      column="UAltTS"
      length="14"
    />

    <property
      name="UAltUtilizador"
      type="java.lang.Integer"
      column="UAltUtilizador"
      length="11"
    />

    <!-- Associations -->

    <!-- Many to One -->

    <!-- One to Many -->

    <set
      name="relOM_ASGrupo_ASUtilizador"
      inverse="true">
      <key column="ChvEASGrupo"/>
      <one-to-many class="pt.gedi.siag.persistence.ASUtilizador"/>
    </set>

    <set
      name="relOM_ASGrupo_ASGrupoPrivilegio"
      inverse="true"
      cascade="all">
      <key column="ChvEASGrupo"/>
      <one-to-many class="pt.gedi.siag.persistence.ASGrupoPrivilegio"/>
    </set>

    <set
      name="relOM_ASGrupo_ASUtilizadorGrupo"
      inverse="true"
      cascade="all">
      <key column="ChvEASGrupo"/>
      <one-to-many class="pt.gedi.siag.persistence.ASUtilizadorGrupo"/>
    </set>

    <set
      name="relOM_ASGrupo_PSDocumentoNota"
      inverse="true">
      <key column="ChvEASGrupo"/>
      <one-to-many class="pt.gedi.siag.persistence.PSDocumentoNota"/>
    </set>

</class>
</hibernate-mapping>
