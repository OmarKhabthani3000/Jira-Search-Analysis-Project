<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >

<hibernate-mapping>

  <!--
    Created by GEDI 1.0 28-02-2005 15:20:15
  -->

  <class
    name="pt.gedi.siag.persistence.ASUtilizador"
    table="asutilizador"
    lazy="true">

    <id
      name="chvP"
      type="java.lang.Integer"
      column="ChvP">
      <generator class="native" />
    </id>

    <property
      name="defaultModAvancado"
      type="java.lang.String"
      column="DefaultModAvancado"
      length="40"
    />

    <property
      name="grupoFiltro"
      type="java.lang.Integer"
      column="GrupoFiltro"
      length="11"
    />

    <property
      name="criacaoTS"
      type="java.sql.Timestamp"
      column="CriacaoTS"
      length="14"
      not-null="true"
    />

    <property
      name="criacaoUtilizador"
      type="java.lang.Integer"
      column="CriacaoUtilizador"
      length="11"
      not-null="true"
    />

    <property
      name="UAltTS"
      type="java.sql.Timestamp"
      column="UAltTS"
      length="14"
    />

    <property
      name="UAltUtilizador"
      type="java.lang.Integer"
      column="UAltUtilizador"
      length="11"
    />

    <property
      name="chvEPSUnidadeUtilizadora"
      type="java.lang.Integer"
      column="ChvEPSUnidadeUtilizadora"
      length="11"
      insert="false"
      update="false"
    />

    <property
      name="chvEPSExercicio"
      type="java.lang.Integer"
      column="ChvEPSExercicio"
      length="11"
      insert="false"
      update="false"
    />

    <property
      name="nomeUtilizador"
      type="java.lang.String"
      column="NomeUtilizador"
      length="40"
      not-null="true"
      unique="true"
    />

    <property
      name="codAcesso"
      type="java.lang.String"
      column="CodAcesso"
      length="40"
    />

    <property
      name="chvEASGrupo"
      type="java.lang.Integer"
      column="ChvEASGrupo"
      length="11"
      insert="false"
      update="false"
    />

    <property
      name="numMaquinas"
      type="java.lang.Integer"
      column="NumMaquinas"
      length="11"
    />

    <property
      name="numAcessos"
      type="java.lang.Integer"
      column="NumAcessos"
      length="11"
    />

    <property
      name="ultimoAcesso"
      type="java.sql.Timestamp"
      column="UltimoAcesso"
      length="14"
    />

    <property
      name="tempoUtilizacao"
      type="java.sql.Timestamp"
      column="TempoUtilizacao"
      length="14"
    />

    <property
      name="multiJanela"
      type="java.lang.Integer"
      column="MultiJanela"
      length="11"
      not-null="true"
    />

    <property
      name="fechoJanela"
      type="java.lang.Integer"
      column="FechoJanela"
      length="11"
      not-null="true"
    />

    <property
      name="fechoMenu"
      type="java.lang.Integer"
      column="FechoMenu"
      length="11"
      not-null="true"
    />

    <property
      name="tipoDocExistencia"
      type="java.lang.Integer"
      column="TipoDocExistencia"
      length="11"
      not-null="true"
    />

    <property
      name="maxRegistos"
      type="java.lang.Integer"
      column="MaxRegistos"
      length="11"
    />

    <property
      name="visualizacao"
      type="java.lang.Integer"
      column="Visualizacao"
      length="11"
    />

    <property
      name="defaultTabAuxiliar"
      type="java.lang.String"
      column="DefaultTabAuxiliar"
      length="40"
    />

    <!-- Associations -->

    <!-- Many to One -->

    <many-to-one
      name="relMO_ASUtilizador_PSUnidadeUtilizadora"
      class="pt.gedi.siag.persistence.PSUnidadeUtilizadora"
      column="ChvEPSUnidadeUtilizadora"
    />

    <many-to-one
      name="relMO_ASUtilizador_PSExercicio"
      class="pt.gedi.siag.persistence.PSExercicio"
      column="ChvEPSExercicio"/>

    <many-to-one
      name="relMO_ASUtilizador_ASGrupo"
      class="pt.gedi.siag.persistence.ASGrupo"
      column="ChvEASGrupo" 
      cascade="none"/>

    <!-- One to Many -->

    <set
      name="relOM_ASUtilizador_ASUtilizadorPrivilegio"
      inverse="true" 
      cascade="all">
      <key column="ChvEASUtilizador"/>
      <one-to-many  class="pt.gedi.siag.persistence.ASUtilizadorPrivilegio"/>
    </set>

    <set
      name="relOM_ASUtilizador_ASUtilizadorGrupo"
      inverse="true"
      cascade="all-delete-orphan"
      lazy="false">
      <key column="ChvEASUtilizador"/>
      <one-to-many class="pt.gedi.siag.persistence.ASUtilizadorGrupo"/>
    </set>
  
    <set
      name="relOM_ASUtilizador_SIListas"
      inverse="true"
      cascade="all">
      <key column="ChvEASUtilizador"/>
      <one-to-many class="pt.gedi.siag.persistence.SIListas"/>
    </set>

    <set
      name="relOM_ASUtilizador_SIPesquisa"
      inverse="true"
      cascade="all">
      <key column="ChvEASUtilizador"/>
      <one-to-many class="pt.gedi.siag.persistence.SIPesquisa"/>
    </set>

    <set
      name="relOM_ASUtilizador_SIOrdenacao"
      inverse="true"
      cascade="all">
      <key column="ChvEASUtilizador"/>
      <one-to-many class="pt.gedi.siag.persistence.SIOrdenacao"/>
    </set>

    <set
      name="relOM_ASUtilizador_PSDocumentoNota"
      inverse="true">
      <key column="ChvEASUtilizador"/>
      <one-to-many class="pt.gedi.siag.persistence.PSDocumentoNota"/>
    </set>

</class>
</hibernate-mapping>
