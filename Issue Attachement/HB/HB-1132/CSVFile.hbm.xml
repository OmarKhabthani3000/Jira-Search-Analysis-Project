<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping package="biz.c24.io.csv">
   <class name="CSVFile">
      <id name="id">
         <generator class="native"/>
      </id>
      <array name="record">
         <key column="CSVFile_id"/>
         <index column="id"/>
         <one-to-many class="biz.c24.io.csv.Record"/>
      </array>
   </class>
</hibernate-mapping>
