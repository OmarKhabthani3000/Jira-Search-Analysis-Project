<?xml version="1.0" encoding="utf-8" ?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
    <class
        name="art.risiko.Risiko"
        table="T_KO_ART_Risiko"
        dynamic-update="false"
        dynamic-insert="false"

        discriminator-value="Risiko"

        >
        <id
            name="id"
            column="ID"
            type="long">
            <generator class="sequence">
                <param name="sequence">TUKO101.SQ_KO_ART_Risiko</param>
            </generator>
        </id>

    <discriminator column="class"/>        


        <property name="name" update="true" insert="true" column="name" not-null="false" unique="false"/>

        <property name="beschreibung" update="true" insert="true" column="beschreibung" not-null="false" unique="false"/>

        <component name="ersterBearbeiter" class="art.objects.Erfasser" update="true" insert="true">


        <component name="zugehoerigkeit" class="art.objects.OrganisationsEinheit" update="true" insert="true">


        <property name="mandant" update="true" insert="true" column="erster_BEAB_ZGH_mandant" not-null="false" unique="false"/>

        <property name="einheit" update="true" insert="true" column="erster_BEAB_ZGH_einheit" not-null="false" unique="false"/>

        <property name="mandantenName" update="true" insert="true" column="erster_BEAB_ZGH_mandantenName" not-null="false" unique="false"/>

        <property name="einheitName" update="true" insert="true" column="erster_BEAB_ZGH_einheitName" not-null="false" unique="false"/>
                    
        </component>

        <property name="name" update="true" insert="true" column="ersterBearbeitername" not-null="false" unique="false"/>

        <property name="vorname" update="true" insert="true" column="ersterBearbeitervorname" not-null="false" unique="false"/>

        <property name="email" update="true" insert="true" column="ersterBearbeiteremail" not-null="false" unique="false"/>

        <property name="telefonnummer" update="true" insert="true" column="ersterBearbeitertelefonnummer" not-null="false" unique="false"/>
                    
        </component>

        <component name="letzterBearbeiter" class="art.objects.Person" update="true" insert="true">


        <component name="zugehoerigkeit" class="art.objects.OrganisationsEinheit" update="true" insert="true">


        <property name="mandant" update="true" insert="true" column="letzter_BEAB_ZGH_mandant" not-null="false" unique="false"/>

        <property name="einheit" update="true" insert="true" column="letzter_BEAB_ZGH_einheit" not-null="false" unique="false"/>

        <property name="mandantenName" update="true" insert="true" column="letzter_BEAB_ZGH_mandantenName" not-null="false" unique="false"/>

        <property name="einheitName" update="true" insert="true" column="letzter_BEAB_ZGH_einheitName" not-null="false" unique="false"/>
                    
        </component>

        <property name="name" update="true" insert="true" column="letzterBearbeitername" not-null="false" unique="false"/>

        <property name="vorname" update="true" insert="true" column="letzterBearbeitervorname" not-null="false" unique="false"/>

        <property name="email" update="true" insert="true" column="letzterBearbeiteremail" not-null="false" unique="false"/>

        <property name="telefonnummer" update="true" insert="true" column="letzterBearbeitertelefonnummer" not-null="false" unique="false"/>
                    
        </component>

        <subclass name="art.risiko.workshoprisiko.Workshoprisiko" discriminator-value="Workshoprisiko">


        <component name="ansprechpartner" class="art.objects.Erfasser" update="true" insert="true">


        <component name="zugehoerigkeit" class="art.objects.OrganisationsEinheit" update="true" insert="true">


        <property name="mandant" update="true" insert="true" column="WS_risiko_ASP_ZGH_mandant" not-null="false" unique="false"/>

        <property name="einheit" update="true" insert="true" column="WS_risiko_ASP_ZGH_einheit" not-null="false" unique="false"/>

        <property name="mandantenName" update="true" insert="true" column="WS_risiko_ASP_ZGH_MAN_Name" not-null="false" unique="false"/>

        <property name="einheitName" update="true" insert="true" column="WS_risiko_ASP_ZGH_einheitName" not-null="false" unique="false"/>
                    
        </component>

        <property name="name" update="true" insert="true" column="WS_risikoansprechpartnername" not-null="false" unique="false"/>

        <property name="vorname" update="true" insert="true" column="WS_risiko_ASP_vorname" not-null="false" unique="false"/>

        <property name="email" update="true" insert="true" column="WS_risikoansprechpartneremail" not-null="false" unique="false"/>

        <property name="telefonnummer" update="true" insert="true" column="WS_risiko_ASP_telefonnummer" not-null="false" unique="false"/>
                    
        </component>

        <many-to-one name="workshop" class="art.workshop.Workshop" column="workshop_ID" not-null="true"/>
      
        </subclass>

        <subclass name="art.risiko.einzelrisiko.Einzelrisiko" discriminator-value="Einzelrisiko">


        <component name="ansprechpartner" class="art.objects.EinzelrisikoErfasser" update="true" insert="true">


        <component name="zugehoerigkeit" class="art.objects.OrganisationsEinheit" update="true" insert="true">


        <property name="mandant" update="true" insert="true" column="Einzelrisiko_ASP_ZGH_mandant" not-null="false" unique="false"/>

        <property name="einheit" update="true" insert="true" column="Einzelrisiko_ASP_ZGH_einheit" not-null="false" unique="false"/>

        <property name="mandantenName" update="true" insert="true" column="Einzelrisiko_ASP_ZGH_MAN_Name" not-null="false" unique="false"/>

        <property name="einheitName" update="true" insert="true" column="Einzelrisiko_ASP_ZGH_EIN_Name" not-null="false" unique="false"/>
                    
        </component>

        <property name="name" update="true" insert="true" column="Einzelrisiko_ASP_name" not-null="false" unique="false"/>

        <property name="vorname" update="true" insert="true" column="Einzelrisiko_ASP_vorname" not-null="false" unique="false"/>

        <property name="email" update="true" insert="true" column="Einzelrisiko_ASP_email" not-null="false" unique="false"/>

        <property name="telefonnummer" update="true" insert="true" column="Einzelrisiko_ASP_telefonnummer" not-null="false" unique="false"/>
                    
        </component>
      
        </subclass>


    </class>

</hibernate-mapping>