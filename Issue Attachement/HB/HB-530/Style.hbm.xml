<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping
        SYSTEM "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
    <class name="com.enterra.surv.po.Style" table="t_style" proxy="com.enterra.surv.po.Style">
        <cache usage="read-write"/>

        <id name="id" column="f_style_id" type="int" unsaved-value="0">
            <generator class="com.enterra.surv.system.IdGenerator"/>
        </id>

        <property name="name" type="java.lang.String" column="f_name" not-null="true"/>

        <property name="path" type="java.lang.String" column="f_style_path" not-null="true"/>

        <property name="archived" type="boolean" column="f_archived"/>

        <property name="styleType" type="com.enterra.surv.po.enum.StyleType" column="f_style_type" not-null="true" />

    </class>
</hibernate-mapping>

