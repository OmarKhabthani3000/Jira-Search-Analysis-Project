<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping
	PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
	<class name="test.AObject" table="AObject">		
		<id name="id" type="integer" unsaved-value="null">
			<generator class="identity"/>
		</id>
		<!-- A one-to-one relationship using a foreign-key is mapped as a 
			many-to-one with a uniqueness contraint -->
		<many-to-one name="oneToOne" class="test.BObject"
			unique="true" column="oneToOne_id"/>
		
		<!-- A many-to-many association -->
		<set name="manyToMany" table="AObject_BObject_ManyToMany" lazy="true">
			<key>
				<column name="a_id" not-null="true"/>
			</key>
			<many-to-many class="test.BObject">
				<column name="b_id" not-null="true"/>
			</many-to-many>
		</set>
	</class>
</hibernate-mapping>
