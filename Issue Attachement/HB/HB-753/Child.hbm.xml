<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
    <class name="hu.pmmedia.test.hibernate.Child" table="KINDERLEISTUNGSART">
		<composite-id>
			<key-property name="erfassungsbeginn" type="calendar" column="erfassungsbeginn"/>
			<key-property name="beginndatum" type="calendar" column="beginndatum"/>
			<key-property name="instanz" type="string" column="instanz"/>
		</composite-id>
        <property name="name" type="string"/>
    </class>
</hibernate-mapping>