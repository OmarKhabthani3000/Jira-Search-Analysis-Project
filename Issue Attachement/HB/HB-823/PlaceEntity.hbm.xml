<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
    <class
        name="si.marand.maf.server.building.module.entity.PlaceEntity"
        table="Place"
        dynamic-update="false"
        dynamic-insert="false"
    >
        <cache usage="read-write" />

        <id
            name="id"
            column="Place_ID"
            type="java.lang.Long"
        >
            <generator class="si.marand.maf.server.common.persistent.hibernate.id.SequenceHiLoGenerator">
                <param name="sequence">Place_ID</param>
            </generator>
        </id>

        <version
            name="mafVersion"
            type="java.lang.Long"
            column="MAF_Version"
        />

        <property
            name="placeTypeId"
            type="java.lang.Long"
            update="true"
            insert="true"
            column="Place_Type_ID"
        />

        <property
            name="note"
            type="java.lang.String"
            update="true"
            insert="true"
            column="Note"
        />

        <many-to-one
            name="placeCategory"
            class="si.marand.maf.server.building.module.entity.PlaceCategoryEntity"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            column="Place_Category_ID"
        />

        <set
            name="siteSet"
            lazy="true"
            inverse="true"
            cascade="all"
            sort="natural"
        >
            <cache 
                usage="read-write" 
             />

              <key
                  column="Place_ID"
              />

              <one-to-many
                  class="si.marand.maf.server.building.module.entity.SiteEntity"
              />
        </set>

        <property
            name="code"
            type="java.lang.String"
            update="true"
            insert="true"
            column="Code"
        />

        <property
            name="name"
            type="java.lang.String"
            update="true"
            insert="true"
            column="Name"
        />

        <property
            name="foreignName"
            type="java.lang.String"
            update="true"
            insert="true"
            column="Foreign_Name"
        />

        <property
            name="mafDeleted"
            type="java.lang.Long"
            update="true"
            insert="true"
            column="MAF_Deleted"
        />

        <component
            name="auditInfo"
            class="si.marand.maf.server.common.entity.core.entity.AuditInfo"
        >
        <property
            name="mafCreatorId"
            type="java.lang.Long"
            update="true"
            insert="true"
            column="MAF_Creator_ID"
        />

        <property
            name="mafCreateTimestamp"
            type="si.marand.maf.server.common.persistent.hibernate.type.TimestampType"
            update="true"
            insert="true"
            column="MAF_Create_Timestamp"
        />

        <property
            name="mafEditorId"
            type="java.lang.Long"
            update="true"
            insert="true"
            column="MAF_Editor_ID"
        />

        <property
            name="mafEditTimestamp"
            type="si.marand.maf.server.common.persistent.hibernate.type.TimestampType"
            update="true"
            insert="true"
            column="MAF_Edit_Timestamp"
        />

        </component>

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-PlaceEntity.xml
            containing the additional properties and place it in your merge dir.
        -->

        <joined-subclass
            name="si.marand.maf.server.building.module.entity.LocalPlaceEntity"
            table="Local_Place"
            dynamic-update="false"
            dynamic-insert="false"
            proxy="si.marand.maf.server.building.module.entity.LocalPlaceEntity"
        >
        <key
            column="Local_Place_ID"
        />
        <property
            name="apartmentNo"
            type="java.lang.String"
            update="true"
            insert="true"
            column="Apartment_No"
        />

        <property
            name="floor"
            type="java.lang.Integer"
            update="true"
            insert="true"
            column="Floor"
        />

        <property
            name="formalLocalPlace"
            type="boolean"
            update="true"
            insert="true"
            column="Formal_Local_Place"
        />

        <many-to-one
            name="entrance"
            class="si.marand.maf.server.building.module.entity.EntranceEntity"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            column="Entrance_ID"
        />

        <set
            name="fromRelationshipSet"
            lazy="true"
            inverse="true"
            cascade="all"
            sort="natural"
        >
            <cache 
                usage="read-write" 
             />

              <key
                  column="Local_Place_ID"
              />

              <one-to-many
                  class="si.marand.maf.server.building.module.entity.LocalPlaceRelationshipEntity"
              />
        </set>

        <set
            name="toRelationshipSet"
            lazy="true"
            inverse="true"
            cascade="all"
            sort="natural"
        >
            <cache 
                usage="read-write" 
             />

              <key
                  column="Related_Local_Place_ID"
              />

              <one-to-many
                  class="si.marand.maf.server.building.module.entity.LocalPlaceRelationshipEntity"
              />
        </set>

        </joined-subclass>
        <joined-subclass
            name="si.marand.maf.server.building.module.entity.EntranceEntity"
            table="Entrance"
            dynamic-update="false"
            dynamic-insert="false"
            proxy="si.marand.maf.server.building.module.entity.EntranceEntity"
        >
        <key
            column="Entrance_ID"
        />

        <set
            name="buildingEntranceSet"
            lazy="true"
            inverse="true"
            cascade="all"
            sort="natural"
        >
            <cache 
                usage="read-write" 
             />

              <key
                  column="Entrance_ID"
              />

              <one-to-many
                  class="si.marand.maf.server.building.module.entity.BuildingEntranceEntity"
              />
        </set>

        <set
            name="serviceProviderCodeSet"
            lazy="true"
            inverse="true"
            cascade="all"
            sort="natural"
        >
            <cache 
                usage="read-write" 
             />

              <key
                  column="Entrance_ID"
              />

              <one-to-many
                  class="si.marand.maf.server.building.module.entity.ServiceProviderCodeEntity"
              />
        </set>

        <set
            name="entranceAddressTimelineSet"
            lazy="true"
            inverse="true"
            cascade="all"
            sort="natural"
        >
            <cache 
                usage="read-write" 
             />

              <key
                  column="Entrance_ID"
              />

              <one-to-many
                  class="si.marand.maf.server.building.module.entity.EntranceAddressTimelineEntity"
              />
        </set>

        <set
            name="localPlaceSet"
            lazy="true"
            inverse="true"
            cascade="all"
            sort="natural"
            where="MAF_Deleted is null"
        >
            <cache 
                usage="read-write" 
             />

              <key
                  column="Entrance_ID"
              />

              <one-to-many
                  class="si.marand.maf.server.building.module.entity.LocalPlaceEntity"
              />
        </set>

        </joined-subclass>
        <joined-subclass
            name="si.marand.maf.server.building.module.entity.BuildingGroupEntity"
            table="Building_Group"
            dynamic-update="false"
            dynamic-insert="false"
            proxy="si.marand.maf.server.building.module.entity.BuildingGroupEntity"
        >
        <key
            column="Building_Group_ID"
        />

        <set
            name="buildingGroupMemberSet"
            lazy="true"
            inverse="true"
            cascade="all"
            sort="natural"
        >
            <cache 
                usage="read-write" 
             />

              <key
                  column="Building_Group_ID"
              />

              <one-to-many
                  class="si.marand.maf.server.building.module.entity.BuildingGroupMemberEntity"
              />
        </set>

        </joined-subclass>
        <joined-subclass
            name="si.marand.maf.server.building.module.entity.BuildingEntity"
            table="Building"
            dynamic-update="false"
            dynamic-insert="false"
            proxy="si.marand.maf.server.building.module.entity.BuildingEntity"
        >
        <key
            column="Building_ID"
        />
        <property
            name="builtYear"
            type="java.lang.Integer"
            update="true"
            insert="true"
            column="Built_Year"
        />

        <property
            name="rebuiltYear"
            type="java.lang.Integer"
            update="true"
            insert="true"
            column="Rebuilt_Year"
        />

        <property
            name="numberOfFloors"
            type="java.lang.Integer"
            update="true"
            insert="true"
            column="Number_Of_Floors"
        />

        <property
            name="area"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            column="Area"
        />

        <property
            name="height1"
            type="java.lang.Integer"
            update="true"
            insert="true"
            column="Height_1"
        />

        <property
            name="height2"
            type="java.lang.Integer"
            update="true"
            insert="true"
            column="Height_2"
        />

        <many-to-one
            name="buildingClassification"
            class="si.marand.maf.server.building.module.entity.BuildingClassificationEntity"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            column="Building_Classification_ID"
        />

        <many-to-one
            name="buildingMaterial"
            class="si.marand.maf.server.building.module.entity.BuildingMaterialEntity"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            column="Building_Material_ID"
        />

        <set
            name="buildingEntranceSet"
            lazy="true"
            inverse="true"
            cascade="all"
            sort="natural"
        >
            <cache 
                usage="read-write" 
             />

              <key
                  column="Building_ID"
              />

              <one-to-many
                  class="si.marand.maf.server.building.module.entity.BuildingEntranceEntity"
              />
        </set>

        <set
            name="serviceProviderCodeSet"
            lazy="true"
            inverse="true"
            cascade="all"
            sort="natural"
        >
            <cache 
                usage="read-write" 
             />

              <key
                  column="Building_ID"
              />

              <one-to-many
                  class="si.marand.maf.server.building.module.entity.ServiceProviderCodeEntity"
              />
        </set>

        <set
            name="buildingGroupMemberSet"
            lazy="true"
            inverse="true"
            cascade="all"
            sort="natural"
        >
            <cache 
                usage="read-write" 
             />

              <key
                  column="Building_ID"
              />

              <one-to-many
                  class="si.marand.maf.server.building.module.entity.BuildingGroupMemberEntity"
              />
        </set>

        </joined-subclass>

    </class>

</hibernate-mapping>
