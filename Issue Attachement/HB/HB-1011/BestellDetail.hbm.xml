<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE hibernate-mapping PUBLIC
          "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping package="de.jdufner">

  <class name="BestellDetail" table="MG_BestellDetails">
    
            
        
      <composite-id>
                                    
            <key-property name="muenzwert"
                          type="int"
                          column="MUENZWERT"/>
                  
                          
            <key-many-to-one name="bestellKopf"
                             class="de.jdufner.BestellKopf">
              
                <column name="BESTELLDATUM"/>
                            
                <column name="FNR"/>
              
            </key-many-to-one>
                  
      </composite-id>
    
                    
        <property name="anzahl"
                  type="int"
                  column="ANZAHL"/>
                
        <property name="uptimestamp"
                  type="java.util.Date"
                  column="UPTIMESTAMP"/>
                
        <property name="upprog"
                  type="java.lang.String"
                  column="UPPROG"/>
              
                            
  </class>

</hibernate-mapping>

