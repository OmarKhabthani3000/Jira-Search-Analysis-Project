<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE hibernate-mapping PUBLIC
          "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping package="de.jdufner">

  <class name="Filiale" table="MG_FilialAktiv">
    
                                <id name="filialnummer"
              type="int"
              column="FNR">
                  <generator class="assigned"/>
          <!--generator class="sequence">
            <param name="sequence">uid_sequence</param>
          </generator-->
        </id>

              
        
                    
        <property name="ort"
                  type="java.lang.String"
                  column="ORT"/>
                
        <property name="aktiv"
                  type="boolean"
                  column="AKTIV_KZ"/>
                
        <property name="uptimestamp"
                  type="java.util.Date"
                  column="UPTIMESTAMP"/>
                
        <property name="upprog"
                  type="java.lang.String"
                  column="UPPROG"/>

                            
  </class>

</hibernate-mapping>

