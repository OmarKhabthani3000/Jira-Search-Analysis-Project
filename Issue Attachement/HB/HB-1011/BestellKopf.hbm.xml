<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE hibernate-mapping PUBLIC
          "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping package="de.jdufner">

  <class name="BestellKopf" table="MG_BestellKopf">
    
            
        
      <composite-id>
                                    
            <key-property name="bestelldatum"
                          type="java.util.Calendar"
                          column="BESTELLDATUM"/>
                  
                          
            <key-many-to-one name="filiale"
                             class="de.jdufner.Filiale">
              
                <column name="FNR"/>
                            
            </key-many-to-one>
                  
      </composite-id>
    
                    
        <property name="exportnummer"
                  type="int"
                  column="DATEINUMMER"/>
                
        <property name="lieferdatum"
                  type="java.util.Calendar"
                  column="LIEFERDATUM"/>
                
        <property name="bestellwert"
                  type="double"
                  column="BESTELLWERT"/>
                
        <property name="uptimestamp"
                  type="java.util.Date"
                  column="UPTIMESTAMP"/>
                
        <property name="upprog"
                  type="java.lang.String"
                  column="UPPROG"/>
	              
                                                      
            <set name="bestellDetail"
                 inverse="true"
                 lazy="true">
              <key>
                                  
                  <column name="BESTELLDATUM"/>
                  
                                  
                  <column name="FNR"/>
                  
              </key>
              <one-to-many class="de.jdufner.BestellDetail"/>
            </set>
                            
  </class>

</hibernate-mapping>

