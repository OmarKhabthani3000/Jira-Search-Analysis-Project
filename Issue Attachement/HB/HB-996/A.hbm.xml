<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >

<hibernate-mapping>
<class
    name="A"
    table="A"
>

    <composite-id name="comp_id" class="APK">
        <key-property
            name="a"
            column="A"
            type="java.lang.String"
            length="10"
        />
        <key-property
            name="b"
            column="B"
            type="int"
            length="10"
        />
    </composite-id>

    <property
        name="c"
        type="int"
        column="C"
        length="10"
    />

    <!-- Associations -->
    <many-to-one
        name="b"
        class="B"
        insert="false"
        update="false"
    >
        <column name="B" />
    </many-to-one>

</class>
</hibernate-mapping>
